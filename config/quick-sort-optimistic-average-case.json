{
  "title": "Liczba wykonanych operacji w algorytmie Quick Sort\n dla przypadku średniego oraz optymistycznego\n w zależności od rozmiaru tablicy",
  "output": "quick-sort-optimistic-average-case",
  "prefix": "test0",
  "grid": "(1, 3)",
  "size": "(10.5, 3.5)",
  "invariants": {
    "range": {
      "begin": 100,
      "end": 1000,
      "step": 100
    },
    "repeats": 100
  },
  "plots": [
    {
      "type": "average",
      "metadata": {
        "title": "a) porównania",
        "xlabel": "rozmiar tablicy [n]",
        "ylabel": "liczba porownań [n]",
        "xcolumn": "n",
        "ycolumn": "comparisons",
        "legend": "inside"
      },
      "functions": [
        {
          "label": "$n\\log{}n$",
          "expression": "nlog(n)"
        }
      ],
      "sortings": [
        {
          "algorithm": "quick_sort",
          "label": "p. średni",
          "generator": "random"
        },
        {
          "algorithm": "quick_sort_middle_pivot_selector",
          "label": "p. optymistyczny",
          "generator": "sorted"
        }
      ]
    },
    {
      "type": "average",
      "metadata": {
        "title": "b) zamiana miejsc",
        "xlabel": "rozmiar tablicy [n]",
        "ylabel": "liczba zamian miejsc [n]",
        "xcolumn": "n",
        "ycolumn": "swaps",
        "legend": "inside"
      },
      "functions": [
        {
          "label": "$n\\log{}n$",
          "expression": "nlog(n)"
        }
      ],
      "sortings": [
        {
          "algorithm": "quick_sort",
          "label": "p. średni",
          "generator": "random"
        },
        {
          "algorithm": "quick_sort_middle_pivot_selector",
          "label": "p. optymistyczny",
          "generator": "sorted"
        }
      ]
    },
    {
      "type": "average",
      "metadata": {
        "title": "c) przypisania",
        "xlabel": "rozmiar tablicy [n]",
        "ylabel": "liczba przypisań [n]",
        "xcolumn": "n",
        "ycolumn": "assigns",
        "legend": "inside"
      },
      "functions": [
        {
          "label": "$n$",
          "expression": "n"
        }
      ],
      "sortings": [
        {
          "algorithm": "quick_sort",
          "label": "p. średni",
          "generator": "random"
        },
        {
          "algorithm": "quick_sort_middle_pivot_selector",
          "label": "p. optymistyczny",
          "generator": "sorted"
        }
      ]
    }
  ]
